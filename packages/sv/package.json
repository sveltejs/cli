{
	"name": "sv",
	"version": "0.12.0",
	"type": "module",
	"description": "A command line interface (CLI) for creating and maintaining Svelte applications",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sveltejs/cli.git",
		"directory": "packages/cli"
	},
	"homepage": "https://svelte.dev",
	"scripts": {
		"check": "tsgo",
		"format": "pnpm lint --write",
		"lint": "prettier --check . --config ../../prettier.config.js --ignore-path ../../.gitignore --ignore-path .gitignore --ignore-path ../../.prettierignore"
	},
	"files": [
		"dist"
	],
	"bin": "./dist/bin.mjs",
	"exports": {
		".": {
			"types": "./dist/lib/index.d.mts",
			"default": "./dist/lib/index.mjs"
		},
		"./testing": {
			"types": "./dist/lib/testing.d.mts",
			"default": "./dist/lib/testing.mjs"
		}
	},
	"devDependencies": {
		"@clack/prompts": "1.0.0",
		"@sveltejs/sv-utils": "workspace:*",
		"@types/estree": "^1.0.8",
		"@types/gitignore-parser": "^0.0.3",
		"@types/ps-tree": "^1.1.6",
		"@types/tar-fs": "^2.0.4",
		"commander": "^14.0.2",
		"create-vite": "^8.2.0",
		"dedent": "^1.7.0",
		"empathic": "^2.0.0",
		"gitignore-parser": "^0.0.2",
		"magic-string": "^0.30.21",
		"package-manager-detector": "^1.6.0",
		"ps-tree": "^1.2.0",
		"sucrase": "^3.35.1",
		"svelte": "^5.47.1",
		"tar-fs": "^3.1.1",
		"tiny-glob": "^0.2.9",
		"tinyexec": "^1.0.2",
		"valibot": "^1.2.0",
		"zimmerframe": "^1.1.4"
	},
	"keywords": [
		"create",
		"new",
		"project",
		"starter",
		"svelte",
		"sveltekit",
		"template",
		"wizard"
	]
}
